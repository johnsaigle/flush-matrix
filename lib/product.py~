class Product:
    def __int__(material_code, name, family_group, density, viscosity_range, viscosity_specs_at_40, viscosity_specs_at_100, sensitivity=None):
        self.material_code = int(material_code) # a numerical product code
        self.family_group = family_group
        self.density= int(density) # physical property of product
        self.viscosity_range = viscosity_range # tuple type: Low, High, Typ
        self.viscosity_specs_at_40 = viscosity_specs_at_40 # tuple type: Low, High, Typ
        self.viscosity_specs_at_100 = viscosity_specs_at_100 # tuple type: Low, High, Typ 
        self.sensitivity = sensitivty # strings representing special notes about the product
